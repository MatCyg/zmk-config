#define SYMBOL_KEY(key) \
    ZMK_MACRO( \
        sym_##key, \
        wait-ms = <0>; \
        tap-ms = <0>; \
        bindings \
            = <&macro_release &kp LALT> \
            , <&macro_tap &to ALPHA> \
            , <&macro_tap &tog SYMBOL> \
            , <&macro_press &kp key> \
            , <&macro_pause_for_release> \
            , <&macro_release &kp key> \
            ; \
)

SYMBOL_KEY(SPC)

